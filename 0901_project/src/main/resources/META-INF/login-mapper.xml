<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
	PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
	"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="LoginMapper">

	<select id="login" parameterType="kr.co.ictedu.util.dto.MemberDTO"
						resultType="kr.co.ictedu.util.dto.MemberDTO">
	select * from member
	where mid = #{mid} and mpwd = #{mpwd}
	</select>
	
	<select id="kakao_login" parameterType="kr.co.ictedu.util.dto.Kakao_MemberDTO" resultType="kr.co.ictedu.util.dto.MemberDTO">
	select * from member
	where mname = #{nickname} and memail = #{email}
	</select>

</mapper>
